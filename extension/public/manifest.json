{
    "manifest_version": 3,
    "name": "Stalk Me",
    "version": "1.0",
    "description": "Tracks URLs and sends them to the local activityTracker engine.",
    "permissions": [
        "tabs", "storage"
    ],
    "host_permissions": [
        "http://localhost:5000/*"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Stalk Me",
        "default_icon": "icon.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}